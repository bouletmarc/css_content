"SpriteCard"
{
	$BASETEXTURE "particle/tinyFireSprites/tinyFireSprites"
	
	// Set this if you always want "additive over" blending (ONE:INVSRCALPHA)
	$ADDOVERBLEND 1
	
// RAMPTEXTURE and POWERFUNCTION are mutually exclusive:	
//	$RAMPTEXTURE "particle/fireGradient"
	$POWERFUNCTION "[0.8 2.5 5]"
	
	$OVERBRIGHTFACTOR 1.5
//	$EXTRACTGREENALPHA 1
	$ADDSELF 0.2
	$DEPTHBLEND 1
}